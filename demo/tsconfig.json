{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "outDir": "./dist-demo",
      "rootDir": "../",
      "baseUrl": ".",
      "paths": {
        "rembg-web": ["../src/index.ts"],
        "rembg-web/*": ["../src/*"]
      },
      "noEmit": false,
      "target": "es2020",
      "module": "esnext",
      "lib": ["es2020", "DOM", "DOM.Iterable"]
    },
    "include": [
      "**/*"
    ],
    "exclude": [
      "node_modules",
      "dist",
      "dist-demo"
    ]
  }